{
  "expo": {
    "name": "AI CornCare",
    "slug": "mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splashIconNew.png",
    "scheme": "mobileapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1b5e20",
        "foregroundImage": "./assets/images/android-icon-foreground.png"
      },
      "edgeToEdgeEnabled": true,
      "splash": {
        "image": "./assets/splashIconNew.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
  "expo-router",
  [
    "expo-splash-screen",
    {
      "image": "./assets/splashIconNew.png",
      "imageWidth": 250,
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    }
  ]
  ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}